{
    "manifest_version": 3,
    "name": "HTML Injector",
    "version": "1.0",
    "description": "Injects HTML onto a page after it loads.",
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ],
            "run_at": "document_idle"
        }
    ]
}